      </Text>
      <ImageView fx:id="exagroaloe" fitHeight="110.0" fitWidth="200.0" layoutX="32.0" layoutY="63.0" onMouseClicked="#exagroaloeOnMouseClicked" pickOnBounds="true">
         <image>
            <Image url="@Assets/img/exagroaloe.png" />
         </image>
      </ImageView>
      <ImageView fx:id="biofert" fitHeight="124.0" fitWidth="200.0" layoutX="313.0" layoutY="63.0" onMouseClicked="#biofertOnMouseClicked" pickOnBounds="true">
