<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.upchiapas.terrainscan.controllers.TiendaController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="816.0" opacity="0.55" pickOnBounds="true">
         <image>
            <Image url="@Assets/img/Login1.jpg" />
