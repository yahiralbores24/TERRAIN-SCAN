            <Font name="Baskerville Old Face" size="34.0" />
         </font>
      </Text>
      <ImageView fx:id="fertiquim" fitHeight="172.0" fitWidth="186.0" layoutX="32.0" layoutY="77.0" onMouseClicked="#fertiquimOnMouseClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Assets/img/Fertiquim2.png" />
         </image>
      </ImageView>
      <ImageView fx:id="pacifex" fitHeight="104.0" fitWidth="200.0" layoutX="308.0" layoutY="93.0" onMouseClicked="#pacifexOnMouseCliked" pickOnBounds="true">
         <image>
            <Image url="@Assets/img/Pacifex.png" />
         </image>
      </ImageView>
      <ImageView fx:id="agrogen" fitHeight="135.0" fitWidth="200.0" layoutX="588.0" layoutY="78.0" onMouseClicked="#agrogenOnMouseClicked" pickOnBounds="true">
         <image>
            <Image url="@Assets/img/Agrogen.png" />
         </image>
      </ImageView>
      <ImageView fx:id="agrifluide" fitHeight="112.0" fitWidth="218.0" layoutX="14.0" layoutY="249.0" onMouseClicked="#agrifluideOnMouseClicked" pickOnBounds="true">
