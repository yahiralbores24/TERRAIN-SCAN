      </ImageView>
      <ImageView fx:id="abssa" fitHeight="150.0" fitWidth="200.0" layoutX="594.0" layoutY="260.0" onMouseClicked="#abssaOnMouseClicked" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Assets/img/Abssa.png" />
         </image>
      </ImageView>
      <ImageView fx:id="fertinal" fitHeight="163.0" fitWidth="188.0" layoutX="314.0" layoutY="260.0" onMouseClicked="#fertinalOnMouseClicked" pickOnBounds="true">
